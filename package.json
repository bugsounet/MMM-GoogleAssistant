{
  "name": "MMM-GoogleAssistant",
  "version": "6.2.6",
  "rev": "240419",
  "description": "An embedded Google Assistant on MagicMirror.",
  "main": "MMM-GoogleAssistant.js",
  "scripts": {
    "preinstall": "installer/preinstall.sh -d 'pulseaudio nginx certbot python3 python-is-python3 python3-certbot-nginx make build-essential'",
    "postinstall": "installer/postinstall.sh -r -b -m",
    "update": "installer/update.sh",
    "rebuild": "installer/rebuild.sh",
    "token": "installer/token.sh",
    "list": "node installer/list.js",
    "delete": "node installer/delete.js",
    "register": "node installer/register.js",
    "reset": "git reset --hard",
    "clean": "rm -rf node_modules package-lock.json",
    "refresh": "node installer/refresh.js",
    "smarthome": "sudo node installer/setup && npm run certbot",
    "certbot": "sudo installer/certbot.sh",
    "project": "installer/check_ProjectID.sh",
    "migrate": "node installer/migrate.js",
    "lint": "eslint *.js installer/*.js components/*.js website/tools/*.js website/config/*/*.js components/AssistantSDK/*.js --config eslint.config.js",
    "lint:fix": "eslint *.js installer/*.js components/*.js website/tools/*.js website/config/*/*.js components/AssistantSDK/*.js --config eslint.config.js --fix",
    "test": "npm run lint",
    "test:minify": "cd installer && node minify.js"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "MMM-GoogleAssistant",
    "magic mirror",
    "module",
    "google assistant",
    "voice",
    "embedded",
    "assistant for device",
    "voice control",
    "smart home"
  ],
  "author": "bugsounet",
  "license": "MIT",
  "bugs": {
    "url": "https://forum.bugsounet.fr/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bugsounet/MMM-GoogleAssistant"
  },
  "homepage": "https://wiki.bugsounet.fr/MMM-GoogleAssistant",
  "dependencies": {
    "@colors/colors": "^1.6.0",
    "@electron/rebuild": "^3.6.0",
    "@grpc/grpc-js": "^1.10.6",
    "actions-on-google": "^3.0.0",
    "cheerio": "^1.0.0-rc.12",
    "cli-spinner": "^0.2.10",
    "esbuild": "^0.20.2",
    "express-session": "^1.18.0",
    "glob": "^10.3.12",
    "google-auth-library": "^9.8.0",
    "html-entities": "^2.5.2",
    "is-valid-domain": "^0.1.6",
    "jsoneditor": "^10.0.2",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "node-html-parser": "^6.1.13",
    "node-pty": "^1.0.0",
    "open": "^10.1.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pm2": "^5.3.1",
    "press-any-key": "^1.0.0",
    "prompt": "^1.3.0",
    "protobufjs": "^7.2.6",
    "readline": "^1.3.0",
    "semver": "^7.6.0",
    "stream": "^0.0.2",
    "systeminformation": "^5.22.7",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@stylistic/eslint-plugin": "^1.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1"
  }
}
