{
  "name": "MMM-GoogleAssistant",
  "version": "7.2.0",
  "rev": "240913",
  "description": "An embedded Google Assistant on MagicMirror.",
  "main": "MMM-GoogleAssistant.js",
  "scripts": {
    "preinstall": "installer/preinstall.sh -d 'pulseaudio python3 python3-pip python-is-python3 make build-essential'",
    "postinstall": "installer/postinstall.sh -m",
    "update": "installer/update.sh",
    "token": "installer/token.sh",
    "list": "node installer/list.js",
    "delete": "node installer/delete.js",
    "register": "node installer/register.js",
    "reset": "git reset --hard",
    "clean": "rm -rf node_modules package-lock.json",
    "refresh": "node installer/refresh.js",
    "lint": "eslint *.js installer/*.js components/*.js components/AssistantSDK/*.js --config eslint.config.js",
    "lint:fix": "eslint *.js installer/*.js components/*.js components/AssistantSDK/*.js --config eslint.config.js --fix",
    "test": "npm run lint",
    "test:minify": "cd installer && node minify.js"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "MMM-GoogleAssistant",
    "magic mirror",
    "module",
    "google assistant",
    "voice",
    "embedded",
    "assistant for device",
    "voice control",
    "smart home"
  ],
  "author": "bugsounet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bugsounet/MMM-GoogleAssistant/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bugsounet/MMM-GoogleAssistant"
  },
  "homepage": "https://wiki.bugsounet.fr/MMM-GoogleAssistant",
  "dependencies": {
    "@grpc/grpc-js": "^1.11.2",
    "buscar.io": "^1.8.1",
    "cli-spinner": "^0.2.10",
    "esbuild": "^0.23.1",
    "glob": "^10.4.5",
    "google-auth-library": "^9.14.1",
    "html-entities": "^2.5.2",
    "mkdirp": "^3.0.1",
    "node-html-parser": "^6.1.13",
    "open": "^10.1.0",
    "pm2": "^5.4.2",
    "press-any-key": "^1.0.0",
    "protobufjs": "^7.4.0",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.1",
    "@stylistic/eslint-plugin": "^1.8.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1"
  }
}
